{
  "name": "IB Results",
  "description": "Makes every result look beautiful!",
  "author": "Leon",
  "version": "0.0.1",
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_title": "Make this page blue"
  },
  "content_scripts": [
   {
     "matches": ["*://example.com/*"],
     "css": ["style.css"],
     "js": ["contentScript.js"],
     "all_frames": true
   }
 ],
  "manifest_version": 2
}
