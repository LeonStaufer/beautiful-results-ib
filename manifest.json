{
  "name": "IB Results",
  "description": "Makes every result look beautiful!",
  "version": "0.0.1",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Make this page blue"
  },
  "content_scripts": [
   {
     "matches": ["*://example.com/*"],
     "css": ["style.css"],
     "js": ["contentScript.js"]
   }
 ],
  "manifest_version": 2
}
