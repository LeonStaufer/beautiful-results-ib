{
  "name": "IB Results",
  "description": "Makes every result look beautiful!",
  "author": "Leon",
  "version": "0.0.2",
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_icon": {
      "38": "beautify.png"
    },
    "default_title": "Beautify results"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://example.com/*",
        "*://candidates.ibo.org/*"
      ],
      "css": [
        "style.css"
      ],
      "js": [
        "contentScript.js"
      ],
      "all_frames": true
    }
  ],
  "manifest_version": 2
}
